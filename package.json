{
  "name": "base-domain",
  "version": "4.6.0",
  "description": "simple module to help build Domain-Driven Design",
  "main": "dist/main",
  "browser": {
    "dist/main": "dist/main-browser"
  },
  "directories": {
    "test": "spec/"
  },
  "bin": {
    "bd-create-master": "bin/create-master-json",
    "bd-generate-entry": "bin/generate-entry"
  },
  "scripts": {
    "mocha": "mocha -r spec/global.js spec/util.coffee spec/master-data-resource.coffee spec/fixture.coffee spec/lib/*.coffee",
    "single": "mocha -r spec/global.js",
    "test": "gulp build && DIST=1 npm run mocha && npm run base-domainify-test && npm run uglify-js-test && npm run generate-entry-test",
    "base-domainify-test": "test/base-domainify/test.sh",
    "uglify-js-test": "rm -rf test/uglify-js/build && gulp uglify-test && node test/uglify-js/build/index.js",
    "generate-entry-test": "test/generate-entry/test.sh"
  },
  "engines": {
    "node": ">=0.12"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/CureApp/base-domain.git"
  },
  "bugs": {
    "url": "https://github.com/CureApp/base-domain/issues"
  },
  "homepage": "https://github.com/CureApp/base-domain",
  "keywords": [
    "Domain",
    "DDD",
    "LoopBack"
  ],
  "author": "CureApp, Inc.",
  "license": "MIT",
  "dependencies": {
    "clone": "^1.0.2",
    "coffee-script": "^1.8.0",
    "commander": "^2.9.0",
    "debug": "^2.2.0",
    "deep-eql": "^0.1.3",
    "through": "^2.3.7"
  },
  "devDependencies": {
    "browserify": "^13.0.1",
    "coffeeify": "^2.0.1",
    "espower-coffee": "^1.0.0",
    "gulp": "^3.9.1",
    "gulp-coffee": "^2.3.1",
    "gulp-uglify": "^1.5.3",
    "gulp-yuidoc": "^0.1.2",
    "mocha": "^2.3.3",
    "node-circleci-autorelease": "^2.1.7",
    "power-assert": "^1.1.0"
  }
}
